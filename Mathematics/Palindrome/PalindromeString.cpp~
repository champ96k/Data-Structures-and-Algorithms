// String Palindrome

#include<iostream>
#include<string.h>
using namespace std;



int main() {

  int t;
  cin>>t;
  while(t--) {
  string str;
  int length;
  cin>>length;
  
  cin>>str;
  
  for(char i=0; i<length;i++) {
    if(str[0]==str[length-1-i]) {
      cout<<"Yes"<<endl;
      break;
    } else {
      cout<<"No"<<endl;
      break;
    }
  }
  
  }
  
  
 return 0; 
}
