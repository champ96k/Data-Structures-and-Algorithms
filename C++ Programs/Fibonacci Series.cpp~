//Fibonacci Series without Recursion


#include<iostream>
using namespace std;

int mian() {

  int n,n1=0,n2=1,sum=0;
  cout<<"Enter the Boundry Number of Fibonacci Series: ";
  cin>>n;
  
  cout<<n1<<" "<<n2<<" ";
  for(int i=0;i<n;i++) {
    sum = n1+n2;
    n1 = i;
    n2 = i+1;
  }

  return 0;
}


