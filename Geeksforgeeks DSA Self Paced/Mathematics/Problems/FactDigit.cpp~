#include<bits/stdc++.h>
using namespace std;


int digitsInFactorial (int N) {
  
  unsigned int res=1;
  int i,count=0;
  
  for(i=1;i<=N;i++) {
    res=res*i;
  }
  
  
  while(res!=0) {
    res=res/10;
    count++;
  }
  
  return count;
  
}



int main()
{
    int T;
    cin>>T;
    while(T--)
    {
        int N;
        cin>>N;
        cout<<digitsInFactorial(N)<<endl;
    }
    return 0;
}
