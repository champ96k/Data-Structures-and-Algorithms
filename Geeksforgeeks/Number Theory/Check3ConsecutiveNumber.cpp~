#include <bits/stdc++.h>
using namespace std;

void consecutiveNumber(int n) {
    if(n%3==0) {
      cout<<n/3-1<<" "<<n/3<<" "<<n/3+1<<endl;
    } else {
      cout<<"-1"<<endl;
    }
}
int main() {
	//code
	int t;
	cin>>t;
	while(t--) {
	    int n;
	    cin>>n;
	    consecutiveNumber(n);
	}
	return 0;
}
