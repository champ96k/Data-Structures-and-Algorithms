#include <bits/stdc++.h>
using namespace std;


int sumUnderModulo(long long a,long long b)
{
    
    int c= a%1000000007;
    int d= b%1000000007;
    int result;
    result=c+d;
    if(result>1000000007)
    {
    result=result%1000000007;
    }
    return result;
}


int main() {
	int T;
	cin>>T;
	while(T--)
	{
	    long long a;
	    long long b;
	    cin>>a>>b;
	    cout<<sumUnderModulo(a,b)<<endl;
	}
	return 0;
}  
