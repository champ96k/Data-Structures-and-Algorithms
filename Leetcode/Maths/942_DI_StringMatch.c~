class Solution {
public:
    vector<int> diStringMatch(string S) {
        
        vector<int> v;
        int l = S.length();
        int small=0,largh=l;
        
        for(int i=0;i<l;i++) {
            if(S[i]=='I') {
                v.push_back(small++);
            }else if(S[i]=='D') {
                v.push_back(largh--);
            }
        }
        
        if(S[l-1] == 'D')
            v.push_back(small);
        else
            v.push_back(largh);
        
        return v;
    }
};
