#include<bits/stdc++.h>
using namespace std;

   int reverse(int x) {
        
        string str = to_string(x);
        
        int size = str.size();
        string temp;
        for(int i=size;i>=0;i--) {
            if(str[size]=='0') {
                continue;
            } else {
                temp[i] = str[i]; 
            }
        }
              int num1 = atoi(temp);
        return num1;
    }

    
int main() {
  
  int n;
  cin>>n;
  cout<<"Number: "<<reverse(n);
  
  return 0;
}
