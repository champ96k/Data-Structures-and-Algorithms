#include<bits/stdc++.h>
using namespace std;

int main() {
 
 string name;
 cout<<"Enter a String :";
 cin.ignore();
 getline(cin,name);
 
 cout<<"Enter String is :"<<name<<endl;
 
 cout<<"----------------"<<endl;
 
 string firstName,secondName;
 cout<<"Enter First Name: ";
 cin>>firstName;
 cout<<"Enter Last Name: ";
 cin>>secondName;
 
 cout<<"Full Name: "<<firstName+secondName<<endl;
 
 
 return 0;
}
