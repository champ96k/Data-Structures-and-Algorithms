#include<iostream>
using namespace std;

int main() {
  char name[] = "Tushar Nikam";
  
  for(char *cp=name; *cp!=0;*cp++) {
    cout<<*cp;
  }
  cout<<endl;
 
  cout<<"\n\n***************\n\n";
  char myname[] = "Hello Folk";
  
  for(char i:myname) {
    cout<<i;
  }
  
  cout<<endl;
  
  return 0;
}
